journal = {Thin-Walled Structures},
keywords = {Composite action,Finite element analysis,Fire resistance,Local buckling,Shear,Steel beam},
pages = {48--57},
title = {{Factors governing onset of local instabilities in fire exposed steel beams}},
url = {http://www.sciencedirect.com/science/article/pii/S026382311500124X http://ac.els-cdn.com/S026382311500124X/1-s2.0-S026382311500124X-main.pdf?{\_}tid=5f4ace9a-5550-11e6-8166-00000aab0f6c{\&}acdnat=1469771616{\_}6f82428e7da30fad5ac6fc6bef97826e},
