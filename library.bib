keywords = {H-TRIS test,Stone wool–insulated sandwich panels,building fire barriers,full-scale experiments,furnace test,intermediate scale experiments,severe heat exposures},
number = {4},
pages = {315--341},
title = {{Response of stone wool–insulated building barriers under severe heating exposures}},
