pages = {1554--1566},
publisher = {Elsevier},
title = {{Experiments on sheathed cold-formed steel studs in compression}},
url = {https://www-sciencedirect-com.ezp01.library.qut.edu.au/science/article/pii/S0143974X11001027},
volume = {67},
