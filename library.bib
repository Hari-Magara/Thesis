isbn = {0360-1323},
journal = {Building and Environment},
keywords = {Acoustic analysis,Double-leaf wall,FEA,Finite element method,Fluid-structure interaction (FSI),Sound reduction index},
pages = {202--210},
title = {{Finite element acoustic analysis of a steel stud based double-leaf wall}},
url = {http://www.sciencedirect.com/science/article/pii/S0360132313001662 http://ac.els-cdn.com.ezp01.library.qut.edu.au/S0360132313001662/1-s2.0-S0360132313001662-main.pdf?{\_}tid=5476fc54-d58a-11e6-a506-00000aacb35e{\&}acdnat=1483870258{\_}7dbcaa3af1ccebd3b8ed1d53c60db},
