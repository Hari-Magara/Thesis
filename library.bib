journal = {Fire Safety Journal},
keywords = {Cold-formed steel wall panels,Critical temperature,Load bearing walls,Plasterboards,Standard fire,Steel studs},
pages = {61--80},
title = {{Fire performance of cold-formed steel wall panels and prediction of their fire resistance rating}},
volume = {64},
year = {2014}
}
