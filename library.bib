journal = {Thin-Walled Structures},
keywords = {Angle,Cold-formed steel,Column,Design,Direct Strength Method,Post-buckling},
pages = {142--152},
title = {{Cold-formed steel lipped and plain angle columns with fixed ends}},
url = {http://www.sciencedirect.com/science/article/pii/S0263823114000858},
volume = {80},
year = {2014}
}
