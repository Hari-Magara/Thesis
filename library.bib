title = {{Corner properties of cold-formed steel sections at elevated temperatures}},
url = {http://www.sciencedirect.com/science/article/pii/S0263823106000176 http://ac.els-cdn.com/S0263823106000176/1-s2.0-S0263823106000176-main.pdf?{\_}tid=43f50cfa-5550-11e6-8ce4-00000aab0f6b{\&}acdnat=1469771570{\_}4280b5340ce7cd99c4b0721e30875ecd},
