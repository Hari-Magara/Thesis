issn = {20402317},
keywords = {Cold-formed Steel Studs,Finite Element Analysis,Gypsum Plasterboard,Insulation,Load Bearing LSF Walls,Realistic Design Fire,Standard Fire,Thermal Performance,quteprints:53087},
mendeley-tags = {quteprints:53087},
number = {3},
pages = {261--289},
title = {{Thermal performance of load bearing cold-formed steel walls under fire conditions using numerical studies}},
volume = {5},
year = {2014}
}
