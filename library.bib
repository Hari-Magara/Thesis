journal = {Computers {\&} Structures},
keywords = {Cold-formed steel columns,Constitutive laws at elevated temperatures,Direct Strength Method (DSM),Distortional failure,Lipped channels,Rack-sections},
pages = {47--67},
title = {{DSM to predict distortional failures in cold-formed steel columns exposed to fire: Effect of the constitutive law temperature-dependence}},
