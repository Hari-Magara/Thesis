keywords = {Curling,Finite element analysis,Strength reduction,Thin-walled steel,Two-row bolted connection,Ultimate strength},
number = {11},
pages = {1191--1202},
title = {{Investigation on ultimate strength of thin-walled steel single shear bolted connections with two bolts using finite element analysis}},
url = {http://www.sciencedirect.com/science/article/pii/S0263823109001074 http://ac.els-cdn.com/S0263823109001074/1-s2.0-S0263823109001074-main.pdf?{\_}tid=7f6dcc42-8ab4-11e6-8c8a-00000aab0f27{\&}acdnat=1475642031{\_}b84b5249f657ea273b09049978172708},
