isbn = {0379-7112},
journal = {Fire Safety Journal},
keywords = {Column,Fire,Steel,Thermal bowing,Wall},
pages = {53--69},
title = {{Thermal bowing on steel columns embedded on walls under fire conditions}},
url = {http://www.sciencedirect.com/science/article/pii/S0379711214000563 http://ac.els-cdn.com/S0379711214000563/1-s2.0-S0379711214000563-main.pdf?{\_}tid=5f36f7b4-5779-11e6-a048-00000aab0f01{\&}acdnat=1470009128{\_}24e7b5fa78af8e9442503fbf473db90a},
