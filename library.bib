title = {{Simulation Methodology for Coupled Fire-Structure Analysis: Modeling Localized Fire Tests on a Steel Column}},
url = {http://dx.doi.org/10.1007/s10694-015-0495-9 http://download.springer.com/static/pdf/705/art{\%}253A10.1007{\%}252Fs10694-015-0495-9.pdf?originUrl=http{\%}3A{\%}2F{\%}2Flink.springer.com{\%}2Farticle{\%}2F10.1007{\%}2Fs10694-015-0495-9{\&}token2=exp=1496796482{~}acl={\%}2Fstatic{\%}2Fpdf{\%}2F},
