@article{Chen2019b,
abstract = {At present, fire-resistant boards have become the most common fire protection productions in cold-formed steel (CFS) structures. In this paper, a fire experimental investigation of 12 CFS composite assemblies is carried out and the effects of different boards and insulation material are discussed. For non-load-bearing CFS composite assemblies lined with only GP board on the fire side, significant flexural deflection of CFS frame towards the furnace and local buckling of steel web is observed, due to the thermal bowing and fall off of fire-side GP board. Compared to the GP board, the low-bulk-density calcium-silicate (CS) board is not prone to fall off during the fire exposure. Replacing the fire-side base-layer GP board with CS board generates an effective fire protection on the cavity and ambient-side of assemblies. Therefore, the buckling of steel frame becomes insignificant and the testing fire resistance time of insulation failure is extended by 9 min to 46 min. The thermal insulation performance of aluminum silicate wool is obviously superior to that of rock wool in fire. For the present assemblies using aluminum silicate wool as external insulation instead of rock wool, the fire resistance time of insulation failure is extended by 18 min. Meanwhile, the composite panel and autoclaved lightweight concrete (ALC) board display excellent thermal insulation performance. For the CFS assemblies using aluminum silicate wool as external insulation, GP and CS boards as fire-side composite panel and ALC board as ambient-side base-layer sheathing, the fire resistance time of insulation failure becomes more than 171 min.},
author = {Chen, Wei and Ye, Jihong and Li, Xianyong},
doi = {10.1016/j.jcsr.2019.04.003},
file = {::},
issn = {0143974X},
journal = {Journal of Constructional Steel Research},
keywords = {Aluminum silicate wool external insulation,Autoclaved lightweight concrete board,Cold-formed steel composite assemblies,Fire experiments,Gypsum plasterboard,Low-bulk-density calcium-silicate board},
month = {jul},
pages = {290--305},
publisher = {Elsevier},
title = {{Fire experiments of cold-formed steel non-load-bearing composite assemblies lined with different boards}},
url = {https://www.sciencedirect.com/science/article/pii/S0143974X18308198},
volume = {158},
year = {2019}
}
@article{Lu2011,
abstract = {Cold-formed profiled steel roof sheeting can be directly connected to the top chord of a steel roof truss through self-tapping screws. At ambient temperatures, neither EN 1993-1-8 nor EN 1993-1-3 can be used directly for this type of connection. Besides, no design rules are available in EN 1993-1-2 for designing screwed connections in fire. A 3D Finite Element (FE) model for a single-lap shear screwed connection is developed using ABAQUS software. After the validation by tests, the model is used to predict the ultimate resistance of connections at both ambient and elevated temperatures. Further, the effects of edge and end distances on the connection resistance are investigated. Based on the analyses results, revised design equations for predicting the connection resistance are proposed. The design resistance is calibrated by testing and FE analyses results according to the procedure given in EN 1990 and the partial safety factor is derived.},
author = {Lu, Wei and M{\"{a}}kel{\"{a}}inen, Pentti and Outinen, Jyri and Ma, Zhongcheng},
doi = {http://dx.doi.org/10.1016/j.tws.2011.07.014},
file = {:C$\backslash$:/Mendeley PDF/My Collection.Data/PDF/Lu et al. - 2011 - Design of screwed steel sheeting connection at ambient and elevated temperatures.pdf:pdf},
isbn = {0263-8231},
