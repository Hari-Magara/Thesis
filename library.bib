journal = {Thin-Walled Structures},
keywords = {Cold-formed steel shear wall,Cyclic test,Drift angle limit,Equivalent bracing model,Lateral stiffness,Shear strength},
pages = {146--159},
title = {{Cyclic performance of cold-formed steel shear walls sheathed with double-layer wallboards on both sides}},
url = {http://www.sciencedirect.com/science/article/pii/S0263823115000774 http://ac.els-cdn.com/S0263823115000774/1-s2.0-S0263823115000774-main.pdf?{\_}tid=7bc795ee-5550-11e6-a802-00000aacb35d{\&}acdnat=1469771664{\_}d795ee4b40c29816a29faac72299099b},
