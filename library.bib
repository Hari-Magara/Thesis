keywords = {Finite element analysis,Fire performance,Heat transfer analysis,Light-gauge steel frame wall systems},
month = {dec},
number = {4},
pages = {354--376},
publisher = {Emerald Publishing Limited},
title = {{Investigating the fire performance of LSF wall systems using finite element analyses}},
volume = {8},
year = {2017}
}
