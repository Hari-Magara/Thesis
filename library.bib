number = {1–3},
pages = {81--109},
title = {{Slotted steel studs to reduce thermal bridges in insulated walls}},
url = {http://www.sciencedirect.com/science/article/pii/S0263823198000287 http://ac.els-cdn.com/S0263823198000287/1-s2.0-S0263823198000287-main.pdf?{\_}tid=3f5ed0b8-5546-11e6-8838-00000aab0f26{\&}acdnat=1469767268{\_}d2aa0d86718f1f9216def87062ec486c},
